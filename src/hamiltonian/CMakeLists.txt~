add_library(HamiltonianFuncs
				  append_connections.cpp
				  merge_states.cpp
				  output.cpp
				  runtime.cpp
				  core.cpp
				  grow_configuration_space.cpp					     setup.cpp
				  )
				  
add_subdirectory(io_tools)
target_link_libraries(HamiltonianFuncs IO_Tools)
target_include_directories(HamiltonianFuncs PUBLIC ${CMAKE_CURRENT_LIST_DIR} ${CMAKE_CURRENT_LIST_DIR}/io_tools )
